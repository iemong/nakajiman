@keyframes menuBefore {
  0% {
    transform: translate(-50%, -7px) rotate(0);
  }
  40% {
    transform: translate(-50%, 0) rotate(0);
  }
  60% {
    transform: translate(-50%, 0) rotate(0);
  }
  100% {
    transform: translate(-50%, 0) rotate(45deg);
  }
}

@keyframes menuBeforeReverse {
  0% {
    transform: translate(-50%, 0) rotate(45deg);
  }
  40% {
    transform: translate(-50%, 0) rotate(0);
  }
  60% {
    transform: translate(-50%, 0) rotate(0);
  }
  100% {
    transform: translate(-50%, -7px) rotate(0);
  }
}

